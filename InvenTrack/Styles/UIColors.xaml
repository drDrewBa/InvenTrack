<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!---Background color-->
    <Color x:Key="backgroundColorGreen1">#042A2B</Color>
    <Color x:Key="backgroundColorGreen2">#0A383A</Color>
    <Color x:Key="backgroundColorWhite">#FFFFFF</Color>
    
    <!---Window border color-->
    <Color x:Key="winBorderColor1">#2CA58D</Color>
    <Color x:Key="winBorderColor2">#6279B8</Color>
    
    <!---Color palette-->
    <Color x:Key="color1">#042A2B</Color>
    <Color x:Key="color2">#92D1C3</Color>
    <Color x:Key="color3">#F1DEDE</Color>
    <Color x:Key="color4">#6279B8</Color>
    <Color x:Key="color5">#2CA58D</Color>

    <!--Panel over-->
    <Color x:Key="panelOver">#0F3435</Color>
    
    <!--Control button colors-->
    <Color x:Key="close1">#DD5151</Color>
    <Color x:Key="close2">#D52121</Color>
    <Color x:Key="minimize1">#28AEED</Color>
    <Color x:Key="minimize2">#278BEF</Color>

</ResourceDictionary>